CREATE TABLE Books (
    author VARCHAR,
    book_name VARCHAR,
    available BOOL,
    price DOUBLE,
    id INT,
);

CREATE TABLE Characters (
    character VARCHAR,
    author_id INT,
    id INT PRIMARY KEY,
);

